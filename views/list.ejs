<%- include('header') -%>

    <h1><%= listTitle %></h1>
    <ul>
        <% items.forEach((item) => { %>
            <li> <%= item %> </li>
        <% }) %>
    </ul>

    <form action="/" method="post">
        <input type="text" name="newItem" placeholder="New Item">
        <br>
        <button type="submit" name="list" value=<%= listTitle %>>Add</button>
    </form>

<%- include('footer') -%>